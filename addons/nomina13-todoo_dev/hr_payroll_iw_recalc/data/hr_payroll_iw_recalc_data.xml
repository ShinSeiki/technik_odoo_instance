<odoo>
    <data noupdate="1">

        <record id="hr_payslip_iw_recalc_sequence" model="ir.sequence">
            <field name="name">Hr Payslip Iw Recalc</field>
            <field name="code">hr.payslip.iw.recalc.sequence</field>
            <field name="prefix">REC-</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">3</field>
        </record>

        <record id="hr_recalc_lines_sequence" model="ir.sequence">
            <field name="name">Hr Recalc Lines</field>
            <field name="code">hr.recalc.lines.sequence</field>
            <field name="prefix">RECEMP-</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">3</field>
        </record>

    </data>
</odoo>
