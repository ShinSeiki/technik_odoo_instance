<odoo>
	<record id="product_template_form_view_pos_product_no" model="ir.ui.view">
        <field name="name">product.template.pos.product.no</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="/form/sheet/div[@class='oe_title']/div[@name='options']" position="inside">
                <field name="is_number_note"/>
                <label for="is_number_note"/>
            </xpath>
    	</field>
   </record>
</odoo>