#. module: odoo_whatsapp_integration
#: model:mail.template,body_html:odoo_whatsapp_integration.whatsapp_invoice_template
msgid ""
"\n"
"            <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px;\">\n"
"        <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                <div>\n"
"                    Dear *${object.partner_id.name}*\n"
"                </div>\n"
"                <div>\n"
"                    Here is your invoice *${object.name}* with amount *${format_amount(object.amount_total, object.currency_id)}*\n"
"                    from ${object.company_id.name}.\n"
"                </div>\n"
"                <div>\n"
"                    % if object.invoice_payment_state == 'paid':\n"
"                        This invoice is already paid. Amount due is *${format_amount(object.amount_residual,object.currency_id)}*\n"
"                    % else:\n"
"                        Please remit payment at your earliest convenience. Amount due is *${format_amount(object.amount_residual,object.currency_id)}*\n"
"                    % endif\n"
"                </div>\n"
"                The invoice date and time is \"${object.invoice_date}\"\n"
"                <div>\n"
"                    Your order details are as follows:\n"
"                    <br>\n"
"                    % for each in object:\n"
"                        % for id in each.invoice_line_ids:\n"
"                            *Product: ${id.product_id.name}* <br>\n"
"                            *Qty: ${id.quantity}* <br>\n"
"                            *Price: ${id.price_unit}* <br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"                If you have any questions, please feel free to contact us.\n"
"        \n"
"        "
msgstr ""
"\n"
"            <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px;\">\n"
"        <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                <div>\n"
"                    Estimado *${object.partner_id.name}*\n"
"                </div>\n"
"                <div>\n"
"                    Aquí está tu factura. *${object.name}* con cantidad *${format_amount(object.amount_total, object.currency_id)}*\n"
"                    de  ${object.company_id.name}.\n"
"                </div>\n"
"                <div>\n"
"                    % if object.invoice_payment_state == 'paid':\n"
"                        Esta factura ya está pagada. El monto adeudado es *${format_amount(object.amount_residual,object.currency_id)}*\n"
"                    % else:\n"
"                        Por favor remita el pago en el menor tiempo posible. El monto adeudado es *${format_amount(object.amount_residual,object.currency_id)}*\n"
"                    % endif\n"
"                </div>\n"
"                La fecha y hora de la factura es \"${object.invoice_date}\"\n"
"                <div>\n"
"                    Los detalles de su pedido son los siguientes :\n"
"                    <br>\n"
"                    % for each in object:\n"
"                        % for id in each.invoice_line_ids:\n"
"                            *Producto: ${id.product_id.name}* <br>\n"
"                            *Cantidad: ${id.quantity}* <br>\n"
"                            *Presio: ${id.price_unit}* <br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"               Si tiene alguna pregunta, no dude en contactarnos.\n"
"        \n"
"        "

#. module: odoo_whatsapp_integration
#: model:mail.template,body_html:odoo_whatsapp_integration.whatsapp_purchase_template
msgid ""
"<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px;\">\n"
"             <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"             <div>\n"
"                Hello *${object.partner_id.name or ''}*,\n"
"             </div>\n"
"            % if object.state == 'draft' or object.state == 'sent':\n"
"                This is about Request For Quotation (RFQ) *${object.name}* with amount *${format_amount(object.amount_total, object.currency_id)}*\n"
"                <div>\n"
"                    Quotation details are as follows: <br>\n"
"                    % for each in object:\n"
"                        % for id in each.order_line:\n"
"                            *Product: ${id.product_id.name}* <br>\n"
"                            *Qty: ${id.product_qty}* <br>\n"
"                            *Price: ${id.price_subtotal}*<br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"            % else:\n"
"                <div>\n"
"                    Your Purchase Order Number *${object.name}* with amount *${format_amount(object.amount_total, object.currency_id)}* is Confirmed.\n"
"                </div>\n"
"                <div>\n"
"                    Order confirmation date and time is \"${object.date_approve}\"\n"
"                </div>\n"
"                <div>\n"
"                    Order details are as follows: <br>\n"
"                    % for each in object:\n"
"                        % for id in each.order_line:\n"
"                            *Product: ${id.product_id.name}* <br>\n"
"                            *Qty: ${id.product_qty}* <br>\n"
"                            *Price: ${id.price_subtotal}*<br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"            % endif\n"
"            <div>\n"
"                We are expecting receipt of products on or before scheduled date <br/>\n"
"                If you have any questions, please feel free to contact us.\n"
"            </div>\n"
"        \n"
"        "
msgstr ""
"<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px;\">\n"
"             <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"             <div>\n"
"                Hola *${object.partner_id.name or ''}*,\n"
"             </div>\n"
"            % if object.state == 'draft' or object.state == 'sent':\n"
"                Se trata de la Solicitud de Cotización (RFQ) *${object.name}* con cantidad *${format_amount(object.amount_total, object.currency_id)}*\n"
"                <div>\n"
"                    Los detalles de la cotización son los siguientes: <br>\n"
"                    % for each in object:\n"
"                        % for id in each.order_line:\n"
"                            *Productos: ${id.product_id.name}* <br>\n"
"                            *Cantidad: ${id.product_qty}* <br>\n"
"                            *Precio: ${id.price_subtotal}*<br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"            % else:\n"
"                <div>\n"
"                    Su número de orden de compra *${object.name}* con cantidad *${format_amount(object.amount_total, object.currency_id)}* Está confirmada.\n"
"                </div>\n"
"                <div>\n"
"                    La fecha y hora de confirmación del pedido es \"${object.date_approve}\"\n"
"                </div>\n"
"                <div>\n"
"                    Los detalles del pedido son los siguientes:<br>\n"
"                    % for each in object:\n"
"                        % for id in each.order_line:\n"
"                            *Producto: ${id.product_id.name}* <br>\n"
"                            *Cantidad: ${id.product_qty}* <br>\n"
"                            *Precio: ${id.price_subtotal}*<br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"            % endif\n"
"            <div>\n"
"                Esperamos la recepción de los productos en la fecha programada o antes. <br/>\n"
"                Si tiene alguna pregunta, no dude en contactarnos..\n"
"            </div>\n"
"        \n"
"        "

